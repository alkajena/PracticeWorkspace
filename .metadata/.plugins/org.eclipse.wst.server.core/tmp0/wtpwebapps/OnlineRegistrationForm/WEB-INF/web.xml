<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0" metadata-complete="true">
  <display-name>Archetype Created Web Application</display-name>
   
  
  <servlet>
  	<servlet-name>OnlineRegistrationForm</servlet-name>
  	<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
  	<init-param>
  		<param-name>contextConfigLocation</param-name>
  		<param-value>WEB-INF/application-context.xml</param-value>
  	</init-param>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>OnlineRegistrationForm</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  
   <welcome-file-list>
        <welcome-file>signIn.jsp</welcome-file>
    </welcome-file-list>
</web-app>
